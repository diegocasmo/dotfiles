export ZSH=$HOME/.oh-my-zsh
ZSH_THEME="pure"
DISABLE_AUTO_UPDATE="true"
HIST_STAMPS="yyyy-mm-dd"
ZSH_CUSTOM=$HOME/.0/zsh
plugins=(brew composer cp docker-compose docker github gitignore golang gulp history httpie kubectl laravel5 nmap npm pip z)
source $ZSH/oh-my-zsh.sh
